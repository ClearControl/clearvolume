<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>../../build/libs/ClearVolume-1.0-SNAPSHOT.jar</jar>
  <outfile>../../build/executable/ClearVolumeWin/ClearVolume.exe</outfile>
  <errTitle>Please install the latest Java (version 8)</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>http://clearvolume.bitbucket.org</supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>true</restartOnCrash>
  <manifest></manifest>
  <icon>../../artwork/ClearVolumeWinIcon.ico</icon>
  <jre>
    <path>./jre</path>
    <bundledJre64Bit>true</bundledJre64Bit>
    <minVersion>1.7.0</minVersion>
    <maxVersion>1.8.9</maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64</runtimeBits>
    <initialHeapPercent>10</initialHeapPercent>
    <maxHeapPercent>90</maxHeapPercent>
    <opt>-Djava.library.path=dlls</opt>
  </jre>
  <splash>
    <file>../../artwork/SplashScreen.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
</launch4jConfig>